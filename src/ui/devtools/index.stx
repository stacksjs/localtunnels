  <script type="module">
    // Sample data
    const sampleRequests = [
      { id: 1, method: 'GET', path: '/', status: 200, size: '4.2 KB', time: '12ms', ts: '10:42:03', headers: { 'Host': 'swift-fox.localtunnel.dev', 'User-Agent': 'Mozilla/5.0', 'Accept': 'text/html', 'Accept-Encoding': 'gzip, deflate, br', 'Connection': 'keep-alive', 'Cache-Control': 'no-cache' }, body: null, response: '<!DOCTYPE html><html>...</html>' },
