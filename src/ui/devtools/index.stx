<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>localtunnels - DevTools</title>
  <link rel="preconnect" href="https://fonts.googleapis.com">
  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600;700&family=JetBrains+Mono:wght@400;500&display=swap" rel="stylesheet">

  <script>
    const version = '0.2.7'
  </script>

  <style>
    *, *::before, *::after {
      margin: 0;
      padding: 0;
      box-sizing: border-box;
    }

    :root {
      --bg-0: #09090b;
      --bg-1: #0f0f12;
      --bg-2: #18181b;
      --bg-3: #1e1e22;
      --bg-hover: #27272a;
      --border: #27272a;
      --border-light: #3f3f46;
      --text-0: #fafafa;
      --text-1: #a1a1aa;
      --text-2: #71717a;
      --text-3: #52525b;
      --green: #22c55e;
      --green-dim: #16a34a;
      --green-bg: rgba(34, 197, 94, 0.1);
      --green-border: rgba(34, 197, 94, 0.2);
      --blue: #3b82f6;
      --blue-bg: rgba(59, 130, 246, 0.1);
      --blue-border: rgba(59, 130, 246, 0.2);
      --yellow: #eab308;
      --yellow-bg: rgba(234, 179, 8, 0.1);
      --yellow-border: rgba(234, 179, 8, 0.2);
      --red: #ef4444;
      --red-bg: rgba(239, 68, 68, 0.1);
      --red-border: rgba(239, 68, 68, 0.2);
      --purple: #a855f7;
      --purple-bg: rgba(168, 85, 247, 0.1);
      --orange: #f97316;
      --cyan: #06b6d4;
      --mono: 'JetBrains Mono', monospace;
      --sans: 'Inter', -apple-system, BlinkMacSystemFont, sans-serif;
      --radius: 8px;
      --radius-sm: 6px;
    }

    html, body {
      height: 100%;
      background: var(--bg-0);
      color: var(--text-0);
      font-family: var(--sans);
      font-size: 13px;
      line-height: 1.5;
      overflow: hidden;
    }

    ::selection {
      background: var(--blue);
      color: white;
    }

    /* Layout */
    .devtools {
      height: 100vh;
      display: grid;
      grid-template-rows: 48px 1fr;
      grid-template-columns: 260px 1fr 320px;
      grid-template-areas:
        "header header header"
        "sidebar main inspector";
    }

    /* Header */
    .header {
      grid-area: header;
      display: flex;
      align-items: center;
      padding: 0 16px;
      background: var(--bg-1);
      border-bottom: 1px solid var(--border);
      gap: 12px;
      user-select: none;
    }

    .header-brand {
      display: flex;
      align-items: center;
      gap: 8px;
      font-weight: 600;
      font-size: 14px;
      color: var(--text-0);
    }

    .header-brand svg {
      width: 20px;
      height: 20px;
    }

    .header-badge {
      font-size: 10px;
      font-weight: 500;
      color: var(--green);
      background: var(--green-bg);
      border: 1px solid var(--green-border);
      padding: 2px 8px;
      border-radius: 99px;
      text-transform: uppercase;
      letter-spacing: 0.5px;
    }

    .header-sep {
      width: 1px;
      height: 20px;
      background: var(--border);
      margin: 0 4px;
    }

    .header-status {
      display: flex;
      align-items: center;
      gap: 6px;
      font-size: 12px;
      color: var(--text-1);
    }

    .status-dot {
      width: 8px;
      height: 8px;
      border-radius: 50%;
      background: var(--green);
      box-shadow: 0 0 6px var(--green);
    }

    .status-dot.offline {
      background: var(--red);
      box-shadow: 0 0 6px var(--red);
    }

    .status-dot.connecting {
      background: var(--yellow);
      box-shadow: 0 0 6px var(--yellow);
      animation: pulse 1.5s ease-in-out infinite;
    }

    @keyframes pulse {
      0%, 100% { opacity: 1; }
      50% { opacity: 0.4; }
    }

    .header-spacer { flex: 1; }

    .header-actions {
      display: flex;
      align-items: center;
      gap: 4px;
    }

    .header-btn {
      display: flex;
      align-items: center;
      justify-content: center;
      width: 32px;
      height: 32px;
      background: transparent;
      border: none;
      border-radius: var(--radius-sm);
      color: var(--text-2);
      cursor: pointer;
      transition: all 0.15s;
    }

    .header-btn:hover {
      background: var(--bg-hover);
      color: var(--text-0);
    }

    .header-btn svg {
      width: 16px;
      height: 16px;
    }

    /* Sidebar */
    .sidebar {
      grid-area: sidebar;
      background: var(--bg-1);
      border-right: 1px solid var(--border);
      display: flex;
      flex-direction: column;
      overflow: hidden;
    }

    .sidebar-section {
      padding: 12px;
    }

    .sidebar-section-title {
      font-size: 10px;
      font-weight: 600;
      color: var(--text-3);
      text-transform: uppercase;
      letter-spacing: 0.8px;
      margin-bottom: 8px;
      padding: 0 8px;
    }

    .tunnel-card {
      padding: 10px 12px;
      border-radius: var(--radius-sm);
      cursor: pointer;
      transition: background 0.15s;
      border: 1px solid transparent;
    }

    .tunnel-card:hover {
      background: var(--bg-hover);
    }

    .tunnel-card.active {
      background: var(--bg-3);
      border-color: var(--border-light);
    }

    .tunnel-card-header {
      display: flex;
      align-items: center;
      justify-content: space-between;
      margin-bottom: 4px;
    }

    .tunnel-name {
      font-weight: 600;
      font-size: 13px;
      color: var(--text-0);
    }

    .tunnel-status {
      font-size: 10px;
      font-weight: 500;
      padding: 2px 6px;
      border-radius: 99px;
    }

    .tunnel-status.active {
      color: var(--green);
      background: var(--green-bg);
    }

    .tunnel-status.idle {
      color: var(--yellow);
      background: var(--yellow-bg);
    }

    .tunnel-url {
      font-family: var(--mono);
      font-size: 11px;
      color: var(--text-2);
      margin-top: 2px;
    }

    .tunnel-meta {
      display: flex;
      gap: 12px;
      margin-top: 6px;
      font-size: 11px;
      color: var(--text-3);
    }

    .tunnel-meta-item {
      display: flex;
      align-items: center;
      gap: 4px;
    }

    /* Stats row */
    .stats-row {
      display: grid;
      grid-template-columns: repeat(4, 1fr);
      gap: 1px;
      background: var(--border);
      border-top: 1px solid var(--border);
      border-bottom: 1px solid var(--border);
    }

    .stat-card {
      background: var(--bg-1);
      padding: 12px 16px;
      text-align: center;
    }

    .stat-value {
      font-size: 20px;
      font-weight: 700;
      color: var(--text-0);
      font-family: var(--mono);
    }

    .stat-label {
      font-size: 10px;
      color: var(--text-3);
      text-transform: uppercase;
      letter-spacing: 0.5px;
      margin-top: 2px;
    }

    /* Main content */
    .main {
      grid-area: main;
      display: flex;
      flex-direction: column;
      overflow: hidden;
    }

    .main-tabs {
      display: flex;
      align-items: center;
      padding: 0 16px;
      background: var(--bg-1);
      border-bottom: 1px solid var(--border);
      gap: 2px;
      min-height: 36px;
    }

    .tab {
      padding: 8px 12px;
      font-size: 12px;
      font-weight: 500;
      color: var(--text-2);
      cursor: pointer;
      border-bottom: 2px solid transparent;
      transition: all 0.15s;
      user-select: none;
    }

    .tab:hover {
      color: var(--text-1);
    }

    .tab.active {
      color: var(--text-0);
      border-bottom-color: var(--blue);
    }

    .tab-count {
      font-size: 10px;
      color: var(--text-3);
      background: var(--bg-3);
      padding: 1px 5px;
      border-radius: 99px;
      margin-left: 4px;
    }

    /* Toolbar */
    .toolbar {
      display: flex;
      align-items: center;
      padding: 8px 16px;
      gap: 8px;
      border-bottom: 1px solid var(--border);
      background: var(--bg-0);
    }

    .toolbar-input {
      flex: 1;
      background: var(--bg-2);
      border: 1px solid var(--border);
      border-radius: var(--radius-sm);
      padding: 6px 10px;
      color: var(--text-0);
      font-family: var(--mono);
      font-size: 12px;
      outline: none;
      transition: border-color 0.15s;
    }

    .toolbar-input:focus {
      border-color: var(--blue);
    }

    .toolbar-input::placeholder {
      color: var(--text-3);
    }

    .toolbar-btn {
      display: flex;
      align-items: center;
      gap: 4px;
      padding: 6px 10px;
      background: var(--bg-2);
      border: 1px solid var(--border);
      border-radius: var(--radius-sm);
      color: var(--text-1);
      font-family: var(--sans);
      font-size: 12px;
      cursor: pointer;
      transition: all 0.15s;
      white-space: nowrap;
    }

    .toolbar-btn:hover {
      background: var(--bg-hover);
      color: var(--text-0);
    }

    .toolbar-btn svg {
      width: 14px;
      height: 14px;
    }

    /* Request list */
    .request-list {
      flex: 1;
      overflow-y: auto;
    }

    .request-list::-webkit-scrollbar {
      width: 6px;
    }

    .request-list::-webkit-scrollbar-track {
      background: transparent;
    }

    .request-list::-webkit-scrollbar-thumb {
      background: var(--border);
      border-radius: 3px;
    }

    .request-row {
      display: grid;
      grid-template-columns: 56px 1fr 80px 60px 60px 90px;
      align-items: center;
      padding: 8px 16px;
      border-bottom: 1px solid var(--border);
      cursor: pointer;
      transition: background 0.1s;
      font-size: 12px;
    }

    .request-row:hover {
      background: var(--bg-2);
    }

    .request-row.selected {
      background: var(--blue-bg);
      border-color: var(--blue-border);
    }

    .request-row-header {
      display: grid;
      grid-template-columns: 56px 1fr 80px 60px 60px 90px;
      padding: 6px 16px;
      border-bottom: 1px solid var(--border);
      font-size: 10px;
      font-weight: 600;
      color: var(--text-3);
      text-transform: uppercase;
      letter-spacing: 0.5px;
      background: var(--bg-1);
      user-select: none;
    }

    .method {
      font-family: var(--mono);
      font-weight: 600;
      font-size: 11px;
    }

    .method.get { color: var(--green); }
    .method.post { color: var(--blue); }
    .method.put { color: var(--yellow); }
    .method.patch { color: var(--orange); }
    .method.delete { color: var(--red); }
    .method.options { color: var(--purple); }

    .request-path {
      font-family: var(--mono);
      font-size: 12px;
      color: var(--text-0);
      overflow: hidden;
      text-overflow: ellipsis;
      white-space: nowrap;
    }

    .status-code {
      font-family: var(--mono);
      font-weight: 600;
      font-size: 11px;
    }

    .status-code.s2xx { color: var(--green); }
    .status-code.s3xx { color: var(--blue); }
    .status-code.s4xx { color: var(--yellow); }
    .status-code.s5xx { color: var(--red); }

    .request-size {
      color: var(--text-2);
      font-family: var(--mono);
      font-size: 11px;
    }

    .request-time {
      color: var(--text-2);
      font-family: var(--mono);
      font-size: 11px;
    }

    .request-timestamp {
      color: var(--text-3);
      font-size: 11px;
    }

    /* Inspector panel */
    .inspector {
      grid-area: inspector;
      background: var(--bg-1);
      border-left: 1px solid var(--border);
      display: flex;
      flex-direction: column;
      overflow: hidden;
    }

    .inspector-header {
      display: flex;
      align-items: center;
      justify-content: space-between;
      padding: 10px 16px;
      border-bottom: 1px solid var(--border);
      min-height: 40px;
    }

    .inspector-title {
      font-weight: 600;
      font-size: 12px;
      color: var(--text-0);
    }

    .inspector-close {
      display: flex;
      align-items: center;
      justify-content: center;
      width: 24px;
      height: 24px;
      background: transparent;
      border: none;
      border-radius: 4px;
      color: var(--text-3);
      cursor: pointer;
    }

    .inspector-close:hover {
      background: var(--bg-hover);
      color: var(--text-0);
    }

    .inspector-tabs {
      display: flex;
      padding: 0 16px;
      border-bottom: 1px solid var(--border);
      gap: 2px;
    }

    .inspector-tab {
      padding: 8px 10px;
      font-size: 11px;
      font-weight: 500;
      color: var(--text-2);
      cursor: pointer;
      border-bottom: 2px solid transparent;
      transition: all 0.15s;
      user-select: none;
    }

    .inspector-tab:hover {
      color: var(--text-1);
    }

    .inspector-tab.active {
      color: var(--text-0);
      border-bottom-color: var(--blue);
    }

    .inspector-body {
      flex: 1;
      overflow-y: auto;
      padding: 12px 16px;
    }

    .inspector-body::-webkit-scrollbar {
      width: 6px;
    }

    .inspector-body::-webkit-scrollbar-thumb {
      background: var(--border);
      border-radius: 3px;
    }

    .inspector-group {
      margin-bottom: 16px;
    }

    .inspector-group-title {
      font-size: 10px;
      font-weight: 600;
      color: var(--text-3);
      text-transform: uppercase;
      letter-spacing: 0.5px;
      margin-bottom: 6px;
    }

    .inspector-row {
      display: flex;
      padding: 3px 0;
      font-size: 12px;
      gap: 8px;
    }

    .inspector-key {
      color: var(--text-2);
      min-width: 100px;
      flex-shrink: 0;
    }

    .inspector-value {
      color: var(--text-0);
      font-family: var(--mono);
      font-size: 11px;
      word-break: break-all;
    }

    .inspector-body-content {
      background: var(--bg-0);
      border: 1px solid var(--border);
      border-radius: var(--radius-sm);
      padding: 12px;
      font-family: var(--mono);
      font-size: 11px;
      color: var(--text-1);
      white-space: pre-wrap;
      word-break: break-all;
      line-height: 1.6;
      max-height: 300px;
      overflow-y: auto;
    }

    /* JSON syntax highlighting */
    .json-key { color: var(--cyan); }
    .json-string { color: var(--green); }
    .json-number { color: var(--yellow); }
    .json-bool { color: var(--purple); }
    .json-null { color: var(--text-3); }

    /* Empty state */
    .empty-state {
      flex: 1;
      display: flex;
      flex-direction: column;
      align-items: center;
      justify-content: center;
      color: var(--text-3);
      gap: 8px;
      padding: 40px;
    }

    .empty-state svg {
      width: 40px;
      height: 40px;
      opacity: 0.3;
      margin-bottom: 4px;
    }

    .empty-state-text {
      font-size: 13px;
      text-align: center;
    }

    .empty-state-hint {
      font-size: 11px;
      color: var(--text-3);
    }

    /* Connection info in sidebar */
    .conn-info {
      padding: 12px;
      border-top: 1px solid var(--border);
      margin-top: auto;
    }

    .conn-info-row {
      display: flex;
      justify-content: space-between;
      font-size: 11px;
      padding: 3px 8px;
    }

    .conn-info-label {
      color: var(--text-3);
    }

    .conn-info-value {
      color: var(--text-1);
      font-family: var(--mono);
    }

    /* Live badge */
    .live-badge {
      display: flex;
      align-items: center;
      gap: 4px;
      font-size: 11px;
      font-weight: 600;
      color: var(--red);
      text-transform: uppercase;
      letter-spacing: 0.5px;
    }

    .live-badge::before {
      content: '';
      width: 6px;
      height: 6px;
      border-radius: 50%;
      background: var(--red);
      animation: pulse 1s ease-in-out infinite;
    }

    /* Tag/Badge */
    .badge {
      font-size: 10px;
      font-weight: 500;
      padding: 2px 6px;
      border-radius: 4px;
    }

    .badge-ws {
      color: var(--purple);
      background: var(--purple-bg);
    }

    .badge-http {
      color: var(--blue);
      background: var(--blue-bg);
    }

    /* Bottom bar */
    .bottom-bar {
      grid-column: 1 / -1;
      display: flex;
      align-items: center;
      padding: 0 16px;
      height: 28px;
      background: var(--bg-2);
      border-top: 1px solid var(--border);
      font-size: 11px;
      color: var(--text-3);
      gap: 16px;
    }

    .bottom-bar-item {
      display: flex;
      align-items: center;
      gap: 4px;
    }

    /* WebSocket messages view */
    .ws-message {
      display: flex;
      gap: 8px;
      padding: 6px 16px;
      border-bottom: 1px solid var(--border);
      font-size: 12px;
      font-family: var(--mono);
      align-items: flex-start;
    }

    .ws-direction {
      font-size: 10px;
      font-weight: 600;
      padding: 1px 4px;
      border-radius: 3px;
      flex-shrink: 0;
      margin-top: 2px;
    }

    .ws-direction.outgoing {
      color: var(--green);
      background: var(--green-bg);
    }

    .ws-direction.incoming {
      color: var(--blue);
      background: var(--blue-bg);
    }

    .ws-payload {
      color: var(--text-1);
      font-size: 11px;
      word-break: break-all;
      flex: 1;
    }

    .ws-time {
      color: var(--text-3);
      font-size: 10px;
      flex-shrink: 0;
      margin-top: 2px;
    }

    /* Responsive */
    @media (max-width: 1100px) {
      .devtools {
        grid-template-columns: 220px 1fr 0;
        grid-template-areas:
          "header header"
          "sidebar main";
      }
      .inspector { display: none; }
    }

    @media (max-width: 768px) {
      .devtools {
        grid-template-columns: 1fr;
        grid-template-areas:
          "header"
          "main";
      }
      .sidebar { display: none; }
    }
  </style>
</head>
<body>
  <div class="devtools" id="app">
    <!-- Header -->
    <div class="header">
      <div class="header-brand">
        <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
          <path d="M12 2L2 7l10 5 10-5-10-5z"/>
          <path d="M2 17l10 5 10-5"/>
          <path d="M2 12l10 5 10-5"/>
        </svg>
        localtunnels
      </div>
      <span class="header-badge">DevTools</span>
      <div class="header-sep"></div>
      <div class="header-status">
        <span class="status-dot" id="status-dot"></span>
        <span id="status-text">Connected</span>
      </div>
      <div class="header-spacer"></div>
      <div class="live-badge">Live</div>
      <div class="header-sep"></div>
      <div class="header-actions">
        <button class="header-btn" title="Clear requests" id="btn-clear">
          <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <circle cx="12" cy="12" r="10"/>
            <line x1="4.93" y1="4.93" x2="19.07" y2="19.07"/>
          </svg>
        </button>
        <button class="header-btn" title="Toggle recording" id="btn-record">
          <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <circle cx="12" cy="12" r="5" fill="var(--red)"/>
          </svg>
        </button>
        <button class="header-btn" title="Settings" id="btn-settings">
          <svg viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
            <circle cx="12" cy="12" r="3"/>
            <path d="M19.4 15a1.65 1.65 0 0 0 .33 1.82l.06.06a2 2 0 0 1 0 2.83 2 2 0 0 1-2.83 0l-.06-.06a1.65 1.65 0 0 0-1.82-.33 1.65 1.65 0 0 0-1 1.51V21a2 2 0 0 1-2 2 2 2 0 0 1-2-2v-.09A1.65 1.65 0 0 0 9 19.4a1.65 1.65 0 0 0-1.82.33l-.06.06a2 2 0 0 1-2.83 0 2 2 0 0 1 0-2.83l.06-.06A1.65 1.65 0 0 0 4.68 15a1.65 1.65 0 0 0-1.51-1H3a2 2 0 0 1-2-2 2 2 0 0 1 2-2h.09A1.65 1.65 0 0 0 4.6 9a1.65 1.65 0 0 0-.33-1.82l-.06-.06a2 2 0 0 1 0-2.83 2 2 0 0 1 2.83 0l.06.06A1.65 1.65 0 0 0 9 4.68a1.65 1.65 0 0 0 1-1.51V3a2 2 0 0 1 2-2 2 2 0 0 1 2 2v.09a1.65 1.65 0 0 0 1 1.51 1.65 1.65 0 0 0 1.82-.33l.06-.06a2 2 0 0 1 2.83 0 2 2 0 0 1 0 2.83l-.06.06a1.65 1.65 0 0 0-.33 1.82V9a1.65 1.65 0 0 0 1.51 1H21a2 2 0 0 1 2 2 2 2 0 0 1-2 2h-.09a1.65 1.65 0 0 0-1.51 1z"/>
          </svg>
        </button>
      </div>
    </div>

    <!-- Sidebar -->
    <div class="sidebar">
      <div class="sidebar-section">
        <div class="sidebar-section-title">Active Tunnels</div>

        <div class="tunnel-card active" data-tunnel="swift-fox">
          <div class="tunnel-card-header">
            <span class="tunnel-name">swift-fox</span>
            <span class="tunnel-status active">active</span>
          </div>
